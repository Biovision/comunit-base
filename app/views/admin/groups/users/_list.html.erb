<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |entity| %>
          <% if entity.deleted? %>
              <li class="deleted">
                <%= t(:deleted_entity) %>, id: <%= entity.id %>
                (<%= entity.screen_name %>)
              </li>
          <% else %>
              <li>
                <%= render partial: 'admin/groups/users/user', locals: { entity: entity, group: group } %>
              </li>
          <% end %>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>
