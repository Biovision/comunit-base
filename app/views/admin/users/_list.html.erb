<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |user| %>
          <% if user.deleted? %>
              <li class="deleted"><%= "#{t(:deleted_entity)} #{user.screen_name} (#{user.profile_name})" %>, id: <%= user.id %></li>
          <% else %>
              <li><%= render partial: 'admin/users/entity/in_list', locals: { user: user } %></li>
          <% end %>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>
