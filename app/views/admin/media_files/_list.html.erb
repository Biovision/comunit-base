<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |entity| %>
          <% if entity.deleted? %>
              <li class="deleted" data-id="<%= entity.id %>">
                <%= entity.name %>
              </li>
          <% else %>
              <li data-id="<%= entity.id %>">
                <%= render partial: 'admin/media_files/entity/in_list', locals: { entity: entity } %>
              </li>
          <% end %>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>
