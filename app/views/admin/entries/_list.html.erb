<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |entry| %>
          <% if entry.deleted? %>
              <li class="deleted"><%= t(:deleted_entity) %>, id: <%= entry.id %></li>
          <% elsif entry.visible_to? current_user %>
              <li><%= render partial: 'admin/entries/entity/in_list', locals: { entry: entry } %></li>
          <% else %>
              <li><%= entry.model_name.human %> <%= entry.id %></li>
          <% end %>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>